{
  "name": "moira-site",
  "private": true,
  "version": "0.1.0",
  "description": "Website for artist and translator Moira Barrett",
  "author": "Sophie Au <sophie@sophieau.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "prod": "gatsby build && gatsby serve",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "format": "prettier --write '{src,data}/**/*.{tsx,ts}'",
    "clean": "gatsby clean",
    "checks": "tsc && npm run lint",
    "lint": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts' --ignore-pattern 'src/assets/*' && stylelint 'src/**/*' --rd --risd",
    "stylelint": "stylelint '.' --rd --risd",
    "lint:fix": "eslint 'src/**/*.{tsx,ts,js,jsx}' --ignore-pattern '*.d.ts'  --ignore-pattern 'src/assets/*' --fix && stylelint 'src/**/*' --rd --risd --fix"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "gatsby": "^2.19.45",
    "gatsby-image": "^2.3.1",
    "gatsby-plugin-csp": "^1.1.3",
    "gatsby-plugin-linaria": "^2.0.0",
    "gatsby-plugin-manifest": "^2.2.40",
    "gatsby-plugin-netlify-cms": "^4.2.1",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.4.4",
    "gatsby-plugin-sitemap": "^2.2.26",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-remark-images": "^3.1.44",
    "gatsby-source-filesystem": "^2.1.47",
    "gatsby-transformer-remark": "^2.6.50",
    "gatsby-transformer-sharp": "^2.3.13",
    "linaria": "^1.4.0-beta.4",
    "netlify-cms-app": "^2.11.27",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@types/react-helmet": "^5.0.15",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.1",
    "prettier": "2.0.1",
    "stylelint": "^13.2.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "typescript": "^3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SophieAu/spicy-sea-shells-site"
  }
}
